##########Cara install SHC##########

apt-get update

apt-get -y install build-essential

apt-get -y install unzip

wget https://github.com/neurobin/shc/archive/release.zip

unzip release.zip

cd shc-release

./configure

make

make install

cd

cd /root

shc -r -f /root/pentil.sh -o /root/pentilmu

####################################
